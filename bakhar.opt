# edit at your own risk !!
*camera*height: 960 # height of window
*camera*width: 1200 # width of window
*display_F3: false # no description
*font*button*size: 12 # button size
*font*family: FreePixel # name of default font family
*font*size: 16 # font size
*font*title*size: 14 # title size
*language: fr # translation
*pause_when_launched: false # if true, automatically pause the game when launched
*save*automatic: 1 # automatic save every 1 minute
*save*filename: save05.arc # last save
